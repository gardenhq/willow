    "story": "export PROJECT=gardenhq-willow && mocha --recursive -R mocha-multi --reporter-options tap=./test/results/story/mocha.tap,spec=- \"./test/story/**/*.js\""
  },
  "main": "index.js",
  "publishConfig": {}
